#: data/com.github.calo001.fondo.desktop.in:4
#: data/com.github.calo001.fondo.appdata.xml.in:8
msgid "Find the most beautiful wallpapers"
msgstr "Trova gli sfondi più belli"

#: data/com.github.calo001.fondo.desktop.in:5
msgid "com.github.calo001.fondo"
msgstr "com.github.calo001.fondo"

#: data/com.github.calo001.fondo.desktop.in:11
